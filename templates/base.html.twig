<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8">
        <title>{% block title %}Welcome{% endblock %}</title>
        {% block stylesheets %}<link rel="stylesheet" href="/bootstrap/css/bootstrap.css" >{% endblock %}
        <script src="/bootstrap/js/jquery.js"></script>
    </head>
    <body>
    {% include('/inc/navbar.html.twig') %}
    {% block sidebar %} {% endblock %}
    {% for message in app.flashes('success') %}
        <div class="alert alert-success" role="alert">
            {{ message|raw }}
        </div>
    {% endfor %}
    {% for message in app.flashes('error') %}
        <div class="alert alert-danger" role="alert">
            {{ message }}
        </div>
    {% endfor %}
        <div class="container">
            {% block body %}{% endblock %}
        </div>
    {% block javascripts %}<script src="/bootstrap/js/bootstrap.js"></script>
    {% endblock %}
    </body>
</html>
